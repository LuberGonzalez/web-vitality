{
  "name": "web-vitality",
  "type": "project",
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "require": {
    "johnpbloch/wordpress": "6.7.*",
    "wpackagist-plugin/woocommerce": "8.0.*",
    "wpackagist-plugin/woocommerce-gateway-stripe": "9.3.*",
    "wpackagist-plugin/complianz-gdpr": "7.3.*",
    "wpackagist-plugin/wpforms-lite": "1.8.*",
    "wpackagist-plugin/akismet": "5.3.*"
  },
  "extra": {
    "wordpress-install-dir": "wp"
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "johnpbloch/wordpress-core-installer": true
    }
  }
}
